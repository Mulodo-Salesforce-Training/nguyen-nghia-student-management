        <externalId>false</externalId>
        <formula>IF(OR(
         ISBLANK(Map_Location__Latitude__s),
         ISBLANK(Map_Location__Longitude__s)
      ) , &apos;Map preview is not available now&apos;,
 IMAGE(
  &quot;https://maps.googleapis.com/maps/api/staticmap?center=&quot; &amp;
  TEXT(Map_Location__Latitude__s) &amp; &quot;,&quot; &amp; TEXT(Map_Location__Longitude__s) &amp;
  &quot;&amp;zoom=16&amp;size=600x300&amp;&quot; &amp; 
  &quot;markers=color:blue%7Clabel:S%7C&quot; &amp;
  TEXT(Map_Location__Latitude__s) &amp; &quot;,&quot; &amp; TEXT(Map_Location__Longitude__s) &amp;
  &quot;&amp;key=AIzaSyAk4lh1e88wZ542EMBnQWYmAbdan2vtfQM&quot;
,
  &apos;No Map Preview Available&apos;,
  300, 600
))</formula>
