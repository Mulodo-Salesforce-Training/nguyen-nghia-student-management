    <validationRules>
        <fullName>Birthday_Minimum</fullName>
        <active>true</active>
        <description>Candidates age should be greater than 16</description>
        <errorConditionFormula>AND(  
      NOT(ISBLANK(Birthday__c)),
      ((YEAR(TODAY()) - YEAR(Birthday__c)) &lt; 16)
)</errorConditionFormula>
        <errorMessage>Candidates age should be greater than 16</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Digit_Check</fullName>
        <active>true</active>
        <description>Phone should contains only digit and whitespace</description>
        <errorConditionFormula>AND(
    NOT(ISBLANK(Phone__c)),
    NOT(REGEX( Phone__c , &quot;^[0-9 ]+$&quot;))
)</errorConditionFormula>
        <errorMessage>Phone should contains only digit and whitespace</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Suffix_Validation</fullName>
        <active>true</active>
        <description>Email of Candidate only available if it end with suffix @mulodo.com or @gmail.com</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Email__c)),
NOT(OR(REGEX( Email__c ,&quot;\\w([^@\\s,;]+)@(mulodo\\.com)&quot;),
             REGEX( Email__c ,&quot;\\w([^@\\s,;]+)@(gmail\\.com)&quot;))
      )
)</errorConditionFormula>
        <errorMessage>Email of Candidate only available if it end with suffix @mulodo.com or @gmail.com</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Length_Validate</fullName>
        <active>true</active>
        <description>Phone Length should be between 10 and 11 number</description>
        <errorConditionFormula>AND(
   NOT(ISBLANK(Phone__c)),
   OR(
      LEN(SUBSTITUTE(Phone__c,&quot; &quot;,&quot;&quot;)) &lt;10,
      LEN(SUBSTITUTE(Phone__c, &quot; &quot;,&quot;&quot;)) &gt; 11
   )
)</errorConditionFormula>
        <errorMessage>Phone Length should be between 10 and 11 number</errorMessage>
    </validationRules>
